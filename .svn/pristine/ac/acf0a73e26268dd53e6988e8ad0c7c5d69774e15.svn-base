.bodyBox {
	width: 100%;
	background: #fff;
	background-size: 100%;
	float: left;
	clear: left;
}

.banner {
	width: 100%;
	float: left;
	clear: left;
}
.banner {
	width: 100%;
	float: left;
	clear: left;
}

.text {
	width: 88%;
	margin-left: 6%;
	margin-bottom: .4rem;
	float: left;
	clear: left;
}
.text>p {
	width: auto;
	height: .48rem;
	margin-left: .32rem;
	padding: 0 .16rem;
	position: relative;
	z-index: 3;
	float: left;
	clear: left;
}
.text p.title1 {
	width: 3.36rem;
	background: #fff url(../images/title_1.png) no-repeat center center;
	background-size: 3.04rem;
}
.text p.title2 {
	width: 2.08rem;
	background: #fff url(../images/title_2.png) no-repeat center center;
	background-size: 1.76rem;
}
.textBG {
	width: 100%;
	border: 1px solid #f25f0f;
	border-radius: .2rem;
	background-size: 100%;
	padding: .32rem .44rem .2rem;
	position: relative;
	margin-top: -.24rem;
	z-index: 0;
	float: left;
	clear: left;
}
.textBG li {
	width: 100%;
	float: left;
	clear: left;
}
.textBG li span {
	width: 4%;
	font-size: .32rem;
	line-height: .64rem;
	text-align: left;
	color: #fb4a06;
	float: left;
	clear: left;
}
.textBG li p {
	width: 94%;
	font-size: .32rem;
	line-height: .64rem;
	text-align: justify;
	color: #fb4a06;
	float: right;
	clear: none;
}

.from {
	width: 100%;
	border-top: 1px solid #ffdb8f;
	background: #fff7e6;
	padding: .32rem .52rem 0;
	float: left;
	clear: left;
}
.from li {
	width: 100%;
	border-bottom: 1px solid #ffe0a8;
	float: left;
	clear: left;
}
.from li p {
	width: 100%;
	margin-top: .32rem;
	font-size: .28rem;
	line-height: .28rem;
	text-align: left;
	color: #b1aca1;
	float: left;
	clear: left;
}
.from li span {
	width: 100%;
	padding: .2rem 0;
	float: left;
	clear: left;
}
.from li span input {
	width: 100%;
	height: .44rem;
	border: 0;
	background: none;
	font-size: .36rem;
	line-height: .44rem;
	text-align: left;
	color: #333;
	float: left;
	clear: left;
}
.from li span textarea {
	width: 100%;
	height: auto;
	min-height: .44rem;
	border: 0;
	background: none;
	font-size: .36rem;
	line-height: .44rem;
	text-align: left;
	color: #333;
	overflow: visible;
	float: left;
	clear: left;
}
.from li.fromButton {
	width: 100%;
	border: 0;
	padding: .48rem 24.1%;
	float: left;
	clear: left;
}
.from li.fromButton a {
	width: 100%;
	height: 1.12rem;
	background: url(../images/button_apply.png) no-repeat center center;
	background-size: 4.64rem;
	float: left;
	clear: left;
}

.from li.fromButton span {
	width: 100%;
	height: 1.12rem;
	background: url(../images/button_application.png) no-repeat center center;
	background-size: 4.64rem;
	float: left;
	clear: left;
}




























